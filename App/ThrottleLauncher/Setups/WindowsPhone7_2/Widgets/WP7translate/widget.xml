<?xml version="1.0" encoding="utf-8"?>
<panel name="transalte" height="90" width="90">
	<configuration name="Translate"
			description="Google Translate"
			minthrottleversion="1.1"
			comments="thanks eww245"
			createdby="Pth6">
			<variables>
			<var name="General" description="#L:Translate#" default="" type="separator" />
				<var name="defautLang" description="#L:Defaut languager#" default="en" type="String" />
			</variables>
	</configuration>
	<panel name="TileColorWP7translate">
		<icon height="90" width="90" path="#$THEME_BASE#\Images\#$Variable,TColor#.png"/>
