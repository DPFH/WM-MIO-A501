        <icon 
					X="0" Y="0"
					width="90" height="90"
					path="#$THEME_BASE#\Widgets\WP7translate\images\icone.png" 
				/>
		  	    <text 
				    X="10" Y="75" 
				    height="30" width="180" 
				    alignment="Near" 
				    text="Translate" 
				    fontcolor="255,255,255" 
				    fonttype="Segoe WP N Semibold,8,false" 
			    />
			<event type="Click">
				<action execute=":RunProcedure" parameters="ShowHidePanel(RowHome,translate.xml)" />
			</event>
</panel>
